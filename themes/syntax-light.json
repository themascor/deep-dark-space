  "semanticTokenColors": {
    "namespace": { "foreground": "#005faf", "fontStyle": "normal" },
    "class": { "foreground": "#0087af", "fontStyle": "bold" },
    "enum": { "foreground": "#008700", "fontStyle": "underline" },
    "interface": { "foreground": "#005faf", "fontStyle": "bold italic" },
    "struct": { "foreground": "#0087af", "fontStyle": "bold" },
    "typeParameter": { "foreground": "#444444", "fontStyle": "bold" },
    "type": { "foreground": "#91B859", "fontStyle": "bold italic" },
    "parameter": { "foreground": "#444444", "fontStyle": "bold" },
    "variable": { "foreground": "#444444", "fontStyle": "normal" },
    "variable.defaultLibrary": {
      "foreground": "#91B859",
      "fontStyle": "normal"
    },
    "variable.constant": { "foreground": "#91B859", "fontStyle": "normal" },
    "variable.global": { "foreground": "#005f87", "fontStyle": "normal" },
    "property": { "fontStyle": "italic" },
    "property.declaration": { "fontStyle": "italic" },
    "enumMember": { "foreground": "#008700", "fontStyle": "bold" },
    "decorator": { "foreground": "#005faf", "fontStyle": "normal" },
    "function": { "foreground": "#005faf", "fontStyle": "normal" },
    "function.declaration": { "fontStyle": "bold" },
    "method": { "foreground": "#444", "fontStyle": "normal" },
    "method.declaration": {
      "foreground": "#444",
      "fontStyle": "   underline "
    },
    "macro": { "foreground": "#9E7BB0", "fontStyle": "bold" },
    "label": { "foreground": "#005faf", "fontStyle": "normal" },
    "comment": { "foreground": "#878787", "fontStyle": "italic" },
    "string": { "foreground": "#9E7BB0", "fontStyle": "normal" },
    "keyword": { "foreground": "#91B859", "fontStyle": "normal" },
    "keyword.async": { "foreground": "#8700af", "fontStyle": "bold" },
    "keyword.controlFlow": { "foreground": "#8700af", "fontStyle": "bold" },
    "selfKeyword": { "foreground": "#91B859", "fontStyle": "normal" },
    "selfTypeKeyword": { "foreground": "#91B859", "fontStyle": "bold" },
    "operator": { "foreground": "#444444", "fontStyle": "normal" }
  },
  "tokenColors": [
    { "scope": ["strong"], "settings": { "fontStyle": "bold" } },
    { "scope": ["property"], "settings": { "fontStyle": "italic" } },
    {
      "scope": ["keyword"],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": ["keyword.import", "keyword.package"],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": ["keyword.control.at-rule", "keyword.declaration"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": [
        "keyword.control",
        "keyword.control.conditional",
        "keyword.control.flow",
        "punctuation.definition.keyword"
      ],
      "settings": { "foreground": "#8700af", "fontStyle": "bold" }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.bitwise",
        "keyword.operator.word"
      ],
      "settings": { "foreground": "#444444", "fontStyle": "normal" }
    },
    {
      "scope": ["keyword.operator.logical"],
      "settings": { "foreground": "#0087af", "fontStyle": "bold" }
    },
    {
      "scope": ["keyword.operator.expression"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": ["keyword.function", "keyword.operator.new", "keyword.fragment"],
      "settings": { "foreground": "#005f87", "fontStyle": "bold" }
    },
    {
      "scope": ["keyword.control.directive"],
      "settings": { "foreground": "#d70000", "fontStyle": "normal" }
    },
    {
      "scope": ["keyword.type"],
      "settings": { "foreground": "#005f87", "fontStyle": "bold" }
    },
    {
      "scope": ["keyword.other"],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": ["keyword.other.using", "keyword.other.definition"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": ["storage.type"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": [
        "storage.type.built-in.primitive",
        "storage.type.primitive",
        "storage.type.numeric",
        "storage.type.function"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "bold" }
    },
    {
      "scope": ["storage.type.namespace"],
      "settings": { "foreground": "#005f87", "fontStyle": "bold" }
    },
    {
      "scope": ["storage.type.type"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": [
        "storage.type.class",
        "storage.type.struct",
        "storage.type.enum",
        "storage.type.union",
        "storage.type.trait",
        "storage.type.interface",
        "storage.type.impl"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "bold" }
    },
    {
      "scope": ["storage.modifier"],
      "settings": { "foreground": "#005f87", "fontStyle": "bold" }
    },
    {
      "scope": ["storage.modifier.mut"],
      "settings": { "foreground": "#005f87", "fontStyle": "bold" }
    },
    {
      "scope": ["support.function"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": ["support.function.kernel"],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": ["support.function.builtin"],
      "settings": { "foreground": "#9E7BB0", "fontStyle": "bold" }
    },
    {
      "scope": ["support.module", "support.type"],
      "settings": { "foreground": "#444444", "fontStyle": "bold" }
    },
    {
      "scope": [
        "support.class",
        "support.type.primitive",
        "support.type.builtin"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "bold" }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#005f87", "fontStyle": "bold" }
    },
    {
      "scope": ["support.constant"],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": ["entity.name.constant", "entity.name.label"],
      "settings": { "foreground": "#444444", "fontStyle": "normal" }
    },
    {
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "entity.name.struct",
        "entity.name.union",
        "entity.name.class.ford-decl",
        "entity.other.inherited-class",
        "entity.name.function.definition",
        "entity.name.function.constructor",
        "entity.name.function.destructor"
      ],
      "settings": { "foreground": "#0087af", "fontStyle": "bold" }
    },
    {
      "scope": [
        "entity.name.trait",
        "entity.name.interface",
        "entity.name.impl"
      ],
      "settings": { "foreground": "#005faf", "fontStyle": "bold" }
    },
    {
      "scope": ["entity.name.fragment"],
      "settings": { "foreground": "#005f87", "fontStyle": "normal" }
    },
    {
      "scope": [
        "entity.name.type.numeric",
        "entity.name.type.primitive",
        "punctuation.definition.anchor",
        "punctuation.definition.alias",
        "entity.name.type.anchor"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "bold" }
    },
    {
      "scope": ["entity.name.namespace"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": [
        "entity.name.function.macro",
        "entity.name.function.preprocessor"
      ],
      "settings": { "foreground": "#9E7BB0", "fontStyle": "bold" }
    },
    {
      "scope": ["entity.name.enum"],
      "settings": { "foreground": "#008700", "fontStyle": "bold" }
    },
    {
      "scope": ["punctuation.definition.tag", "entity.name.tag"],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": [
        "entity.name.section.group-title",
        "entity.other.attribute-name"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": [
        "punctuation.definition.interpolation",
        "meta.interpolation",
        "punctuation.section.interpolation",
        "meta.interpolated"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": [
        "variable.other",
        "variable.other.readwrite",
        "variable.parameter",
        "variable.other.member",
        "variable.function",
        "variable.annotation",
        "punctuation.definition.annotation"
      ],
      "settings": { "foreground": "#444444", "fontStyle": "normal" }
    },
    {
      "scope": ["variable.other.normal"],
      "settings": { "foreground": "#0087af", "fontStyle": "normal" }
    },
    {
      "scope": ["variable.other.bracket"],
      "settings": { "foreground": "#8700af", "fontStyle": "normal" }
    },
    {
      "scope": [
        "variable.other.constant",
        "variable.other.global",
        "variable.other.readwrite.global"
      ],
      "settings": { "foreground": "#005faf", "fontStyle": "normal" }
    },
    {
      "scope": ["variable.other.property"],
      "settings": { "fontStyle": "italic" }
    },
    {
      "scope": ["variable.language"],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": [
        "variable.other.predefined",
        "variable.other.alias",
        "variable.fragment"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "bold" }
    },
    {
      "scope": [
        "constant",
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.integer.binary",
        "constant.numeric.integer.octal",
        "constant.numeric.integer.decimal",
        "constant.numeric.integer.hexadecimal",
        "constant.numeric.integer.other",
        "constant.numeric.float",
        "constant.numeric.float.binary",
        "constant.numeric.float.octal",
        "constant.numeric.float.decimal",
        "constant.numeric.float.hexadecimal",
        "constant.numeric.float.other",
        "constant.numeric.complex",
        "constant.numeric.complex.real",
        "constant.numeric.complex.imaginary",
        "constant.character.escape",
        "constant.other.placeholder",
        "constant.other"
      ],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": ["constant.language"],
      "settings": { "foreground": "#008700", "fontStyle": "bold" }
    },
    {
      "scope": ["constant.other.reference", "constant.other.reference.link"],
      "settings": { "fontStyle": "underline" }
    },
    {
      "scope": ["string"],
      "settings": { "foreground": "#9E7BB0", "fontStyle": "normal" }
    },
    {
      "scope": [
        "string.unquoted",
        "string.quoted",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple",
        "string.quoted.other",
        "meta.string",
        "string.quoted.unquoted",
        "string.quoted.regexp",
        "source.language-suffix.embedded",
        "string.interpolated",
        "string.regexp"
      ],
      "settings": { "foreground": "#9E7BB0", "fontStyle": "normal" }
    },
    {
      "scope": ["string.unquoted.plain"],
      "settings": { "foreground": "#444444", "fontStyle": "normal" }
    },
    {
      "scope": ["string.other.math"],
      "settings": { "foreground": "#005f87", "fontStyle": "normal" }
    },
    {
      "scope": ["punctuation.definition.link", "string.other.link"],
      "settings": { "fontStyle": "underline" }
    },
    {
      "scope": ["string.interpolated.dollar"],
      "settings": { "foreground": "#0087af", "fontStyle": "normal" }
    },
    {
      "scope": ["comment"],
      "settings": { "foreground": "#878787", "fontStyle": "italic" }
    },
    {
      "scope": [
        "comment.line",
        "comment.block",
        "comment.block.documentation",
        "punctuation.definition.comment",
        "meta.toc-list"
      ],
      "settings": { "foreground": "#878787", "fontStyle": "italic" }
    },
    {
      "scope": ["punctuation.definition.entity"],
      "settings": { "foreground": "#444444", "fontStyle": "normal" }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": { "foreground": "#91B859", "fontStyle": "normal" }
    },
    {
      "scope": [
        "punctuation.definition.list",
        "punctuation.definition.heading"
      ],
      "settings": { "fontStyle": "bold" }
    },
    {
      "scope": ["invalid"],
      "settings": { "foreground": "#d70000", "fontStyle": "normal" }
    },
    {
      "scope": ["invalid.illegal", "invalid.deprecated"],
      "settings": {
        "foreground": "#d70000",
        "fontStyle": "italic underline strikethrough"
      }
    },
    { "scope": ["markup.bold"], "settings": { "fontStyle": "bold" } },
    {
      "scope": ["markup.italic"],
      "settings": { "foreground": "#878787", "fontStyle": "italic" }
    },
    { "scope": ["markup.underline"], "settings": { "fontStyle": "underline" } },
    {
      "scope": ["markup.strikethrough"],
      "settings": { "foreground": "#9E7BB0", "fontStyle": "strikethrough" }
    },
    {
      "scope": ["markup.deleted"],
      "settings": { "foreground": "#d70000", "fontStyle": "strikethrough" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#91B859", "fontStyle": "bold" }
    },
    {
      "scope": ["heading.1"],
      "settings": { "foreground": "#008700", "fontStyle": "bold" }
    },
    {
      "scope": ["heading.2"],
      "settings": { "foreground": "#0087af", "fontStyle": "bold" }
    },
    {
      "scope": ["heading.3"],
      "settings": { "foreground": "#91B859", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.raw.inline", "markup.other", "markup.changed"],
      "settings": { "foreground": "#d70000", "fontStyle": "normal" }
    },
    {
      "scope": ["markup.inserted"],
      "settings": { "foreground": "#008700", "fontStyle": "normal" }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": { "foreground": "#878787", "fontStyle": "italic underline" }
    },
    {
      "scope": [
        "markup.inline.raw",
        "markup.fenced_code.block",
        "markup.raw.block",
        "markup.quote"
      ],
      "settings": { "foreground": "#9E7BB0", "fontStyle": "normal" }
    },
    {
      "scope": ["markup.deleted.diff"],
      "settings": { "foreground": "#af0000" }
    },
    {
      "scope": ["markup.inserted.diff"],
      "settings": { "foreground": "#008700" }
    }
  ]
}
